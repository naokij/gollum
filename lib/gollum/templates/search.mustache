<div id="wiki-wrapper" class="results">
<div id="head">
  <h1>Search Results for <strong>{{query}}</strong></h1>
  <ul class="actions">
    <li class="minibutton">
      {{>searchbar}}
    </li>
    <li class="minibutton" rel='tipsy' title='Home'><a href="{{base_url}}/"
       class="action-home-page"><i class="icon-home"></i></i></a></li>
   {{#signedin}}
   <li class="minibutton jaws" rel='tipsy' title='New Page'>
     <a href="#" id="minibutton-new-page"><i class="icon-plus"></i></a></li>
   {{#allow_uploads}}
   <li class="minibutton jaws" rel='tipsy' title='Upload file'>
     <a href="#" id="minibutton-upload-page"><i class="icon-upload"></i></a></li>
   {{/allow_uploads}}
   <li class="minibutton" rel='tipsy' title='Sign out'>
     <a href="{{base_url}}/logout"><i class='icon-signout'></i></a>
   </li>
   {{/signedin}}
   {{#not_signedin}}
   <li class="minibutton" rel='tipsy' title="Sign in">
     <a href="{{base_url}}/auth/ldap"><i class="icon-signin"></i></a>
   </li>
   {{/not_signedin}}
  </ul>
</div>
<div id="results">

{{#has_results}}
  <ul>
    {{#results}}
    <li>
      <a href="{{base_url}}/{{name}}">{{title}}</a>
      <span class="count">({{count}} matches)</span>
    </li>
    {{/results}}
  </ul>
{{/has_results}}

{{#no_results}}
  <p id="no-results">
    There are no results for your search <strong>{{query}}</strong>.
  </p>
{{/no_results}}

</div>
<div id="footer">
  <ul class="actions">
    <li class="minibutton"><a href="#">Back to Top</a></li>
  </ul>
</div>
</div>

