<div id="wiki-wrapper" class="edit">
<div id="head">
  <h1>Editing <strong>{{title}}</strong></h1>
  <ul class="actions">
    <li class="minibutton" rel='tipsy' title='View Page'><a href="{{base_url}}/{{escaped_url_path}}"
       class="action-view-page"><i class="icon-eye-open"></i></a></li>
    <li class="minibutton" rel='tipsy' title='Page History'><a href="{{base_url}}/history/{{escaped_url_path}}"
       class="action-page-history"><i class="icon-time"></i></a></li>
  </ul>
</div>
  <div id="wiki-content">{{>editor}}</div>
</div>
